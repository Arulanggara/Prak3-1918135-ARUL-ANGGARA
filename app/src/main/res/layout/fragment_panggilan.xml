            <ImageView
                android:layout_width="64dp"
                android:layout_height="67dp"
                android:src="@android:drawable/stat_sys_phone_call_forward" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:layout_marginLeft="70dp"
